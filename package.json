{
  "name": "jovo-inbox",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "setup": "npm run setup:core && npm run setup:api && npm run setup:frontend",
    "setup:api": "cd api && npm i && cd ..",
    "setup:core": "cd core && npm i && cd ..",
    "setup:frontend": "cd frontend && npm i && cd ..",
    "start:dev:api": "cd api && npm run start:dev",
    "start:prod:api": "cd api && npm run start:prod",
    "start:dev:frontend": "cd frontend && npm run serve",
    "start:dev:core": "cd core && npm run watch"
  },
  "author": "jovotech",
  "license": "Apache-2.0"
}
